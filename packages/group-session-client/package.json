    "check": "biome check src/",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "keywords": [
    "groupsession",
    "api",
    "client",
    "japanese",
    "groupware"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.10.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.9.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vite-plugin-dts": "^4.2.4",
    "vitest": "^2.1.4"
  }
}
